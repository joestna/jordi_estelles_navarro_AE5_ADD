<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="migracionDatosBiblioteca">
	<class name="Libro" table="libro">
		<property name="titulo" type="string" column="titulo"/>
		<property name="autor" type="string" column="autor"/>
		<property name="anyoNacimiento" type="string" column="anyo_nacicmiento"/>
		<property name="anyoPublicacion" type="int" column="anyo_publicacion"/>		
		<property name="editorial" type="string" column="editorial"/>
		<property name="numeroPaginas" type="int" column="numero_paginas"/>
		<id name="id" column="identificador"
			<generator class="increment"/>
		</id>
	</class>
</hibernate-mapping>

